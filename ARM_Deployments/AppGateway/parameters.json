{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "applicationGatewayName": {
            "value": "myapp-gateway-01"
        },
        "location": {
            "value": "eastus"
        },
        "virtualNetworkName": {
            "value": "myapp-vnet"
        },
        "subnetName": {
            "value": "appgateway-subnet"
        },
        "subnetAddressPrefix": {
            "value": "10.0.1.0/24"
        },
        "vnetAddressPrefix": {
            "value": "10.0.0.0/16"
        },
        "publicIPName": {
            "value": "myapp-gateway-pip"
        },
        "publicIPAllocationMethod": {
            "value": "Static"
        },
        "publicIPSku": {
            "value": "Standard"
        },
        "applicationGatewaySku": {
            "value": "Standard_v2"
        },
        "capacity": {
            "value": 2
        },
        "autoScaleMinCapacity": {
            "value": 2
        },
        "autoScaleMaxCapacity": {
            "value": 5
        },
        "backendAddresses": {
            "value": [
                {
                    "ipAddress": "10.0.2.4"
                },
                {
                    "ipAddress": "10.0.2.5"
                }
            ]
        },
        "backendPort": {
            "value": 80
        },
        "cookieBasedAffinity": {
            "value": "Disabled"
        },
        "frontendPort": {
            "value": 80
        },
        "protocol": {
            "value": "Http"
        },
        "requestTimeout": {
            "value": 20
        },
        "healthProbeEnabled": {
            "value": true
        },
        "healthProbePath": {
            "value": "/"
        },
        "healthProbeInterval": {
            "value": 30
        },
        "healthProbeTimeout": {
            "value": 30
        },
        "healthProbeUnhealthyThreshold": {
            "value": 3
        },
        "tags": {
            "value": {
                "Environment": "NonProd",
                "Project": "DevOps Templates",
                "Owner": "Infrastructure Team"
            }
        }
    }
}
